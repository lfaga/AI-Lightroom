<?xml version="1.0"?>
<ApiProvider xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<Name>pollinations.ai</Name>
	<Endpoint>https://image.pollinations.ai/prompt/</Endpoint>
	<RequestType>Get</RequestType>
	<!-- Post: json payload, Get: query string -->
	<OutputType>Binary</OutputType>
	<InputSchema>
		<SchemaElement>
			<Name>referrer</Name>
			<Type>Constant</Type>
			<Default>ai-lightroom.lfaga</Default>
			<Required>true</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>nologo</Name>
			<Type>Constant</Type>
			<Default>true</Default>
			<Required>true</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>private</Name>
			<Type>Constant</Type>
			<Default>true</Default>
			<Required>true</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>enhance</Name>
			<Type>Constant</Type>
			<Default>true</Default>
			<Required>true</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>seed</Name>
			<Description>Fixed number for same RNG seed, 0 for random</Description>
			<Type>Integer</Type>
			<Minimum>0</Minimum>
			<Maximum>100000000</Maximum>
			<Step xsi:nil="true"/>
			<Default>0</Default>
			<Required>false</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>prompt</Name>
			<Description>The prompt to instruct the model what to create</Description>
			<Type>String</Type>
			<Minimum xsi:nil="true"/>
			<Maximum xsi:nil="true"/>
			<Step xsi:nil="true"/>
			<Required>true</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>width</Name>
			<Description>Width of the image (multiples of 8)</Description>
			<Type>Integer</Type>
			<Minimum>512</Minimum>
			<Maximum>2048</Maximum>
			<Step>8</Step>
			<Default>512</Default>
			<Required>false</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>height</Name>
			<Description>Height of the image (multiples of 8)</Description>
			<Type>Integer</Type>
			<Minimum>512</Minimum>
			<Maximum>2048</Maximum>
			<Step>8</Step>
			<Default>512</Default>
			<Required>false</Required>
		</SchemaElement>
		<SchemaElement>
			<Name>model</Name>
			<Description>Image generation Model</Description>
			<Type>Choice</Type>
			<Minimum xsi:nil="true"/>
			<Maximum xsi:nil="true"/>
			<Step xsi:nil="true"/>
			<Choices>
				<string>flux</string>
				<string>turbo</string>
				<string>gptimage</string>
			</Choices>
			<Default>flux</Default>
			<Required>true</Required>
		</SchemaElement>
	</InputSchema>
</ApiProvider>
